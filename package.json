{
    "name": "CICDTest",
    "version": "1.0.0",
    "description": "A simple CAP project.",
    "dependencies": {
        "@cap-js/hana": "^2",
        "@sap-cloud-sdk/http-client": "^4.4.0",
        "@sap-cloud-sdk/util": "^4.4.0",
        "@sap/cds": "^9",
        "@sap/low-code-event-handler": "^1.2.5",
        "@sap/xsenv": ">=3.3.2 <3.3.3",
        "@sap/xssec": "^4.12.2",
        "express": "^4",
        "hdb": "^2.27.1",
        "passport": "^0.7.0"
    },
    "devDependencies": {
        "@cap-js/sqlite": "^2",
        "@sap/dev-cap-tools": "^1.31.0"
    },
    "scripts": {
        "start": "cds-serve",
        "start-local": "npm run generate-entry-point && npm run cds-watch",
        "generate-entry-point": "dev-cap-tools gen-entrypoint",
        "cds-watch": "cds watch --open",
        "build": "cds build"
    },
    "private": true,
    "cds": {
        "requires": {
            "[production]": {
                "db": "none",
                "auth": "dummy"
            },
            "auth": {
                "[development]": {
                    "kind": "dummy"
                },
                "[production]": {
                    "kind": "dummy"
                }
            }
        }
    },
    "lcap": true
}